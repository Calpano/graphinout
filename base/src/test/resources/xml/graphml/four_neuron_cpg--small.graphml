<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
			http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

   <!-- Attributes for nodes -->
   <key id="neuron_name" for="node" attr.name="name" attr.type="string">     <default>"neuron"</default>   </key>
   <key id="neuron_type" for="node" attr.name="model" attr.type="string">     <default>leaky</default>   </key>
   <key id="ext_in" for="node" attr.name="is_ext" attr.type="boolean">     <default>true</default>   </key>

   <!-- Attributes for edges -->
   <key id="weight" for="edge" attr.name="weight" attr.type="double"/>

  <graph id="CPG" edgedefault="directed">

   <node id="N1">
	 <data key="neuron_type">leaky</data>
   </node>
   <node id="N2">
	 <data key="neuron_type">leaky</data>
   </node>

   <edge source="N1" target="N2">
      <data key="weight">-5</data>
   </edge>

  </graph>
</graphml>
