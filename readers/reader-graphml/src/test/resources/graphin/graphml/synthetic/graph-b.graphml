<?xml version="1.0" ?>
<graphml>
    <key attr.name="type" attr.type="string" id="type"/>
    <graph edgedefault="directed" id="">
        <node id="s1"><data key="label">Source 1</data></node>
        <node id="t1"><data key="label">Target 1</data></node>
        <node id="t2"><data key="label">Target 2</data></node>

        <edge                  source="s1" target="t1"><data key="type">directed</data></edge>
        <edge directed="false" source="s1" target="t2"><data key="type">undirected</data></edge>
    </graph>
</graphml>
